        if (distance_to_point(obj_player.x,obj_player.y) &lt; 25)
        {
            cash += coin_value;
            scr_textfloat(other.x,other.y,c_moneygreen,"$5");
            audio_play_sound(snd_spark,9,false);
            if (part_system_exists(global.ps))
