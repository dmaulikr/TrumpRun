                part_particles_create(global.spark, other.x, other.y, global.pt_spark, 10);
            }
            instance_destroy();
        }
        mp_potential_step(obj_player.x,obj_player.y,coin_speed,false);
    }
    else
    {
        if (distance_to_object(obj_player) &lt; 30) 
        {
            move_towards_point(obj_player.x,obj_player.y,coin_speed);
        }
    }
}
